		<property name="capacityMargin" value="0.6" />
		<property name="portfolioShares">
			<map>
				<entry>
					<key>
						<ref bean="coalPulverizedSuperCritical" />
					</key>
					<value>0.412</value>
				</entry>
				<entry>
					<key>
						<ref bean="CCGT" />
					</key>
					<value>0.1071</value>
				</entry>
				<entry>
					<key>
						<ref bean="OCGT" />
					</key>
					<value>0.0851</value>
				</entry>
				<entry>
					<key>
						<ref bean="biomassPGT" />
					</key>
					<value>0.0341</value>
				</entry>
				<entry>
					<key>
						<ref bean="nuclearPGT" />
					</key>
					<value>0.0731</value>
				</entry>
				<entry>
					<key>
						<ref bean="windPGT" />
					</key>
					<value>0.1751</value>
