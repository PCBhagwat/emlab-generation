<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Bean post processor to persist created beans -->
	<bean class="agentspring.graphdb.PersistingBeanPostProcessor" />

	<!-- Decarbonization Model -->
	<bean id="decarbonizationModel" class="emlab.domain.agent.DecarbonizationModel"
		p:name="Decarbonization Model" p:simulationLength="${simulationLength}"
		p:co2TradingImplemented="false" p:realRenewableDataImplemented="false"
		p:iterationSpeedFactor="3" p:iterationSpeedCriterion="0.005"
		p:capDeviationCriterion="0.03"
		p:deletionOldPPDPBidsAndCashFlowsEnabled="true" p:deletionAge="5">		
	</bean>

	<bean class="emlab.trend.TriangularTrend" id="demandGrowthTrendDE"
		p:start="${demandGrowthDEStart}" p:min="${demandGrowthDEMin}" p:max="${demandGrowthDEMax}"
		p:top="${demandGrowthDETop}" />

	<!-- Substances -->
	<bean id="coal" class="emlab.domain.technology.Substance" p:name="Coal"
		p:quality="1" p:energyDensity="28000" p:co2Density="3.0" />
	<bean id="naturalgas" class="emlab.domain.technology.Substance"
		p:name="Natural Gas" p:quality="1" p:energyDensity="36" p:co2Density="0.00187" />
	<bean id="biomass" class="emlab.domain.technology.Substance"
		p:name="Biomass" p:quality="0.5" p:energyDensity="25000" p:co2Density="0" />
	<bean id="uranium" class="emlab.domain.technology.Substance"
		p:name="Uranium" p:quality="1" p:energyDensity="3888000000"
		p:co2Density="0" />
	<bean id="electricity" class="emlab.domain.technology.Substance"
		p:name="Electricity" p:quality="1" p:energyDensity="0" p:co2Density="0" />
	<bean id="co2" class="emlab.domain.technology.Substance" p:name="CO2"
		p:quality="1" p:energyDensity="0" p:co2Density="1" />


	<!-- Zones -->
	<bean id="de" class="emlab.domain.gis.Zone" p:name="Country A" />

	<!-- PowerGrid -->
	<bean id="beneluxNode" class="emlab.domain.technology.PowerGridNode">
		<property name="zone" ref="de" />
	</bean>

	<bean id="interconnectorBeneluxGermany" class="emlab.domain.technology.Interconnector"
		p:capacity="${interconnectorCapacity}">
		<property name="connections">
			<set>
				<ref bean="beneluxNode" />
			</set>
		</property>
	</bean>

	<!-- Segments of load duration curves -->
	<bean id="segment1" class="emlab.domain.market.electricity.Segment"
		p:lengthInHours="10" p:segmentID="20" />
	<bean id="segment2" class="emlab.domain.market.electricity.Segment"
