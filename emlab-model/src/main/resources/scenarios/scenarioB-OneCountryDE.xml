					<key>
						<ref bean="photovoltaicPGT" />
					</key>
					<value>0.1091</value>
				</entry>
			</map>
		</property>
		<property name="nodes">
			<set>
				<ref bean="beneluxNode" />
			</set>
		</property>
		<property name="market" ref="beneluxElectricitySpotMarket" />
		<property name="producers">
			<list>
				<ref bean="energyProducerA" />
				<ref bean="energyProducerB" />
				<ref bean="energyProducerC" />
				<ref bean="energyProducerD" />
				<ref bean="energyProducerE" />
			</list>
		</property>
	</bean>

	<!-- CO2 Tax -->
	<bean id="co2TaxTrend" class="emlab.trend.StepTrend" p:start="${co2TaxStart}"
		p:duration="${co2TaxDuration}" p:increment="${co2TaxIncrement}"
		p:minValue="0" />

	<!-- CO2 Cap -->
	<bean id="co2CapTrend" class="emlab.trend.StepTrend" p:start="${co2CapStart}"
		p:duration="${co2CapDuration}" p:increment="${co2CapIncrement}"
		p:minValue="0" />

	<!-- Minimum CO2 Price Trends -->
	<bean id="minCo2PriceTrend" class="emlab.trend.StepTrend" p:start="${minCo2EUPriceStart}"
		p:duration="${minCo2PriceDuration}" p:increment="${minCo2EUPriceIncrement}"
		p:minValue="0" />

	<bean id="minCo2PriceTrendDE" class="emlab.trend.StepTrend"
		p:start="${minCo2DEPriceStart}" p:duration="${minCo2DEPriceDuration}"
		p:increment="${minCo2DEPriceIncrement}" p:minValue="0" />


	<!-- Government -->
	<bean id="government" class="emlab.domain.agent.Government"
		p:name="European Government" p:co2Penalty="500">
		<property name="co2TaxTrend" ref="co2TaxTrend" />
		<property name="co2CapTrend" ref="co2CapTrend" />
		<property name="minCo2PriceTrend" ref="minCo2PriceTrend" />
	</bean>

	<bean id="governmentDE" class="emlab.domain.agent.NationalGovernment"
		p:name="Country A Government">
		<property name="minNationalCo2PriceTrend" ref="minCo2PriceTrendDE" />
		<property name="governedZone" ref="de" />
	</bean>
	
	<bean id="strategicReserveOperator1" class="emlab.domain.agent.StrategicReserveOperator"
		p:name="StrategicReserveOperatorDE" p:cash="0" p:reservePriceSR="3000"
		p:reserveVolumePercentSR="0.1">
		<property name="zone" ref="de" />
	</bean>

