		<property name="segment" ref="segment10" />
	</bean>
	<bean id="segment11de" class="emlab.domain.market.electricity.SegmentLoad"
		p:baseLoad="${segment11de}">
		<property name="segment" ref="segment11" />
	</bean>
	<bean id="segment12de" class="emlab.domain.market.electricity.SegmentLoad"
