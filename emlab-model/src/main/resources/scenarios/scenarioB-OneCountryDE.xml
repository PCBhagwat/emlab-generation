		<property name="fuels">
			<set>
				<ref bean="coal" />
				<ref bean="biomass" />
			</set>
		</property>
	</bean>
	<bean id="IGCC" class="emlab.domain.technology.PowerGeneratingTechnology"
		p:name="IGCC" p:capacity="758" p:efficiency="0.48"
		p:efficiencyModifierExogenous="0.0032068392" p:co2CaptureEffciency="0"
		p:baseInvestmentCost="1373496000" p:investmentCostModifierExogenous="-0.0034744053"
		p:downPayment="412048800" p:depreciationTime="20"
		p:minimumRunningHours="0" p:fixedOperatingCost="48072360"
		p:fixedOperatingCostModifierAfterLifetime=".05" p:expectedLifetime="40"
		p:expectedLeadtime="4" p:expectedPermittime="1" p:minimumFuelQuality=".9"
		p:maximumInstalledCapacityFractionInCountry="1"
		p:maximumInstalledCapacityFractionPerAgent="1"
		p:baseSegmentDependentAvailability="1"
		p:peakSegmentDependentAvailability="1"
		p:applicableForLongTermContract="true" p:intermittent="false">
		<property name="fuels">
			<set>
				<ref bean="coal" />
				<ref bean="biomass" />
			</set>
		</property>
	</bean>
