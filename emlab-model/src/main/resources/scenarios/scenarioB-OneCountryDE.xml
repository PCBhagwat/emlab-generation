		p:name="CoalPulverizedSuperCritical" p:capacity="758" p:efficiency=".44"
		p:efficiencyModifierExogenous="0.000" p:co2CaptureEffciency="0"
		p:baseInvestmentCost="1087351000" p:investmentCostModifierExogenous="0.000"
		p:downPayment="326205300" p:depreciationTime="20"
