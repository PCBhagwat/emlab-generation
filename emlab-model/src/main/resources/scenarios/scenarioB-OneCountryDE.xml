	<!-- Commodity Markets -->
	<bean id="coalMarket" class="emlab.domain.market.CommodityMarket"
		p:auction="false" p:name="Coal market">
		<property name="substance" ref="coal" />
	</bean>

	<bean id="naturalgasMarket" class="emlab.domain.market.CommodityMarket"
		p:auction="false" p:name="Natural gas market">
		<property name="substance" ref="naturalgas" />
	</bean>

	<bean id="biomassMarket" class="emlab.domain.market.CommodityMarket"
		p:auction="false" p:name="Biomass market">
		<property name="substance" ref="biomass" />
	</bean>

	<bean id="nuclearMarket" class="emlab.domain.market.CommodityMarket"
		p:auction="false" p:name="Uranium market">
		<property name="substance" ref="uranium" />
	</bean>

	<!-- CO2 Auction -->
	<bean id="co2Auction" class="emlab.domain.market.CO2Auction"
		p:auction="true" p:referencePrice="0" p:name="CO2 auction"
		p:substance-ref="co2" />

	<!-- Fuel Prices -->
	<bean id="coalPrice" class="emlab.trend.TriangularTrend" p:start="${coalPriceStart}"
		p:min="${coalPriceMin}" p:max="${coalPriceMax}" p:top="${coalPriceTop}" />
	<bean id="gasPrice" class="emlab.trend.TriangularTrend" p:start="${gasPriceStart}"
		p:min="${gasPriceMin}" p:max="${gasPriceMax}" p:top="${gasPriceTop}" />
	<bean id="biomassPrice" class="emlab.trend.TriangularTrend"
		p:start="${biomassPriceStart}" p:min="${biomassPriceMin}" p:max="${biomassPriceMax}"
		p:top="${biomassPriceTop}" />
	<bean id="uraniumPrice" class="emlab.trend.TriangularTrend"
		p:start="${uraniumPriceStart}" p:min="${uraniumPriceMin}" p:max="${uraniumPriceMax}"
		p:top="${uraniumPriceTop}" />

	<!-- Agents -->
	<bean id="coalSupplier" class="emlab.domain.agent.CommoditySupplier">
		<property name="name" value="Coal supplier" />
		<property name="substance" ref="coal" />
		<property name="priceOfCommodity" ref="coalPrice" />
	</bean>

	<bean id="gasSupplier" class="emlab.domain.agent.CommoditySupplier">
		<property name="name" value="Gas supplier" />
		<property name="substance" ref="naturalgas" />
		<property name="priceOfCommodity" ref="gasPrice" />
	</bean>

	<bean id="biomassSupplier" class="emlab.domain.agent.CommoditySupplier">
		<property name="name" value="Biomass supplier" />
		<property name="substance" ref="biomass" />
		<property name="priceOfCommodity" ref="biomassPrice" />
	</bean>

	<bean id="uraniumSupplier" class="emlab.domain.agent.CommoditySupplier">
		<property name="name" value="Uranium supplier" />
		<property name="substance" ref="uranium" />
		<property name="priceOfCommodity" ref="uraniumPrice" />
	</bean>

	<bean id="energyConsumer" class="emlab.domain.agent.EnergyConsumer"
		p:ltcMaximumCoverageFraction="0.8" p:contractDurationPreferenceFactor=".03"
		p:contractWillingnessToPayFactor="1.2">
		<property name="name" value="Energy consumer" />
	</bean>

	<bean id="energyProducerA" class="emlab.domain.agent.EnergyProducer"
		p:name="Energy Producer A" p:cash="${startCash}"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="${investmentFutureTimeHorizon}"
		p:pastTimeHorizon="5" p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="${priceMarkUp}"
		p:numberOfYearsBacklookingForForecasting="${numberOfYearsBacklookingForForecasting}" />
	<bean id="energyProducerB" class="emlab.domain.agent.EnergyProducer"
		p:name="Energy Producer B" p:cash="${startCash}"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="${investmentFutureTimeHorizon}"
		p:pastTimeHorizon="5" p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="${priceMarkUp}"
		p:numberOfYearsBacklookingForForecasting="${numberOfYearsBacklookingForForecasting}" />
	<bean id="energyProducerC" class="emlab.domain.agent.EnergyProducer"
		p:name="Energy Producer C" p:cash="${startCash}"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="${investmentFutureTimeHorizon}"
		p:pastTimeHorizon="5" p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="${priceMarkUp}"
		p:numberOfYearsBacklookingForForecasting="${numberOfYearsBacklookingForForecasting}" />
	<bean id="energyProducerD" class="emlab.domain.agent.EnergyProducer"
		p:name="Energy Producer D" p:cash="${startCash}"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="${investmentFutureTimeHorizon}"
		p:pastTimeHorizon="5" p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="${priceMarkUp}"
		p:numberOfYearsBacklookingForForecasting="${numberOfYearsBacklookingForForecasting}" />
	<bean id="energyProducerE" class="emlab.domain.agent.EnergyProducer"
		p:name="Energy Producer E" p:cash="${startCash}"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="${investmentFutureTimeHorizon}"
		p:pastTimeHorizon="5" p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
