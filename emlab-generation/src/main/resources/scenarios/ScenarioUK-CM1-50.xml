		p:name="Energy Producer D" p:cash="5e9"
		p:investmentRole-ref="standardInvestmentRole"
                p:simpleCapacityMarketEnabled="true" 
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="7" p:pastTimeHorizon="5"
		p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="1.1" 
		p:numberOfYearsBacklookingForForecasting="5"
		p:investorMarket-ref="ukElectricitySpotMarket"/>

		<bean id="standardInvestmentRole" class="emlab.gen.role.investment.InvestInPowerGenerationTechnologiesStandard"/>
    	<bean id="targetInvestmentRole" class="emlab.gen.role.investment.TargetInvestmentRole"/>

        <!-- Renewable Targets -->

        <bean id="renewableTargetInvestorCountryUK" class="emlab.gen.domain.agent.TargetInvestor"
        p:name="Renewable Target Investor UK" p:cash="5e9"
        p:investmentRole-ref="targetInvestmentRole"
        p:priceMarkUp="1.0"
        p:investorMarket-ref="ukElectricitySpotMarket"
        p:equityInterestRate="0.12" p:loanInterestRate="0.09"
        p:debtRatioOfInvestments="0.7">
        <property name="powerGenerationTechnologyTargets">
               <set>
                       <ref bean="windTargetCountryUK"/>
                       <ref bean="pvTargetCountryUK" />
                       <ref bean="windOffshoreTargetCountryUK"/>
                       <ref bean="biomassTargetCountryUK"/>
                       <ref bean="biogasTargetCountryUK"/>
               </set>
        </property>
        </bean>

        <bean id="windTargetCountryUK" class="emlab.gen.domain.policy.PowerGeneratingTechnologyTarget"
        p:trend-ref="windTrendCountryUK" p:powerGeneratingTechnology-ref="windPGT"/>

        <bean id="windTrendCountryUK" class="emlab.gen.trend.TimeSeriesCSVReader" p:filename="/data/policyGoalNREAP_CF_UK-null.csv" p:delimiter="," p:variableName="WindOnshore"/>

        <bean id="windOffshoreTargetCountryUK" class="emlab.gen.domain.policy.PowerGeneratingTechnologyTarget"
        p:trend-ref="windOffshoreTrendCountryUK" p:powerGeneratingTechnology-ref="windOffshorePGT"/>

        <bean id="windOffshoreTrendCountryUK" class="emlab.gen.trend.TimeSeriesCSVReader" p:filename="/data/policyGoalNREAP_CF_UK-null.csv" p:delimiter="," p:variableName="WindOffshore"/>

        <bean id="biomassTargetCountryUK" class="emlab.gen.domain.policy.PowerGeneratingTechnologyTarget"
        p:trend-ref="biomassTrendCountryUK" p:powerGeneratingTechnology-ref="biomassPGT"/>

        <bean id="biomassTrendCountryUK" class="emlab.gen.trend.TimeSeriesCSVReader" p:filename="/data/policyGoalNREAP_CF_UK-null.csv" p:delimiter="," p:variableName="BiomassSolid"/>

        <bean id="pvTargetCountryUK" class="emlab.gen.domain.policy.PowerGeneratingTechnologyTarget"
        p:trend-ref="pvTrendCountryUK" p:powerGeneratingTechnology-ref="photovoltaicPGT"/>

        <bean id="pvTrendCountryUK" class="emlab.gen.trend.TimeSeriesCSVReader" p:filename="/data/policyGoalNREAP_CF_UK-null.csv" p:delimiter="," p:variableName="Photovoltaic"/>

        <bean id="biogasTargetCountryUK" class="emlab.gen.domain.policy.PowerGeneratingTechnologyTarget"
        p:trend-ref="biogasTrendCountryUK" p:powerGeneratingTechnology-ref="biogasPGT"/>

        <bean id="biogasTrendCountryUK" class="emlab.gen.trend.TimeSeriesCSVReader" p:filename="/data/policyGoalNREAP_CF_UK-null.csv" p:delimiter="," p:variableName="Biogas"/>
	
	<!-- Initial portfolios -->

	<bean id="powerPlantManufacturer" class="emlab.gen.domain.agent.PowerPlantManufacturer"
		p:name="Power Plant Manufacturer" />
	<bean id="bigBank" class="emlab.gen.domain.agent.BigBank" p:name="Big Bank" />
	<bean id="powerPlantMaintainer" class="emlab.gen.domain.agent.PowerPlantMaintainer"
		p:name="Power Plant Maintainer" />

        <!-- Power plant factory -->

	<bean id="ukPowerPortfolio" class="emlab.gen.domain.factory.ElectricityProducerFactory">
		<property name="capacityMargin" value="0.35" />
		<property name="portfolioShares">
			<map>
				<entry>
					<key>
						<ref bean="coalPulverizedSuperCritical" />
					</key>
					<value>0.255</value>
