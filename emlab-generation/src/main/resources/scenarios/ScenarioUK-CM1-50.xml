<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Bean post processor to persist created beans -->
	<bean class="agentspring.graphdb.PersistingBeanPostProcessor" />

	<!-- Decarbonization Model -->
        <bean id="decarbonizationModel" class="emlab.gen.domain.agent.DecarbonizationModel"
		p:name="Decarbonization Model" p:simulationLength="40"
		p:co2TradingImplemented="false"
		p:realRenewableDataImplemented="false"
		p:iterationSpeedFactor="3" p:iterationSpeedCriterion="0.005"
		p:simpleCapacityMarketEnabled="true"
		p:capDeviationCriterion="0.03" p:deletionOldPPDPBidsAndCashFlowsEnabled="true"
		p:deletionAge="7">
	</bean>

        <bean id="regulatorCMuk" class="emlab.gen.domain.agent.Regulator"
		p:name="Regulator Capacity Market UK" p:capacityMarketPriceCap="104000"
		p:reserveMargin="0.10" p:targetPeriod="4"
		p:reserveDemandLowerMargin="0.035" p:reserveDemandUpperMargin="0.035"
		p:numberOfYearsLookingBackToForecastDemand="4" p:longTermCapacityContractLengthinYears="15" 
                p:capacityMarketPermittedTimeForConstruction="4"> 
		<property name="zone" ref="uk" />
		</bean>

	<bean id="capacityMarketUK" class="emlab.gen.domain.market.capacity.CapacityMarket"
		p:name="Capacity Market UK" >
		<property name="regulator" ref="regulatorCMuk"/>
		<property name="consumer" ref="energyConsumer"/>
		<property name="zone" ref="uk" />
		</bean>
	
        <bean id="demandGrowthTrendUK" class="emlab.gen.trend.TimeSeriesCSVReader" p:filename="/data/stochasticDemandCWEandGB/demand-50.csv" p:delimiter="," p:variableName="DemandGB" />

	<!-- Substances -->
	<bean id="coal" class="emlab.gen.domain.technology.Substance" p:name="Coal"
		p:quality="1" p:energyDensity="1000" p:co2Density="0.095" p:unit="GJ"  />
	<bean id="lignite" class="emlab.gen.domain.technology.Substance" p:name="Lignite"
		p:quality="1" p:energyDensity="1000" p:co2Density="0.11389" p:unit="GJ" />
	<bean id="naturalgas" class="emlab.gen.domain.technology.Substance"
		p:name="Natural Gas" p:quality="1" p:energyDensity="1000" p:co2Density="0.05194" p:unit="GJ"/>
	<bean id="biomass" class="emlab.gen.domain.technology.Substance"
		p:name="Biomass" p:quality="0.5" p:energyDensity="1000"
		p:co2Density="0" p:unit="GJ" />
	<bean id="uranium" class="emlab.gen.domain.technology.Substance"
		p:name="Uranium" p:quality="1" p:energyDensity="1000"
		p:co2Density="0" p:unit="GJ" />
	<bean id="electricity" class="emlab.gen.domain.technology.Substance"
		p:name="Electricity" p:quality="1" p:energyDensity="0" p:co2Density="0" p:unit="MWh_el" />
	<bean id="co2" class="emlab.gen.domain.technology.Substance" p:name="CO2"
		p:quality="1" p:energyDensity="0" p:co2Density="1" p:unit="tCO2" />

	<!-- Zones -->
	<bean id="uk" class="emlab.gen.domain.gis.Zone" p:name="Country A" />

	<!-- PowerGrid -->
	<bean id="ukNode" class="emlab.gen.domain.technology.PowerGridNode"
	p:capacityMultiplicationFactor="1.0">
		<property name="zone" ref="uk" />
	</bean>

	<bean id="interconnectorUKGermany" class="emlab.gen.domain.technology.Interconnector"
		p:capacity="0">
		<property name="connections">
			<set>
				<ref bean="ukNode" />
			</set>
		</property>
	</bean>

