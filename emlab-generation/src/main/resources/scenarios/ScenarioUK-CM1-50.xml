						<ref bean="OCGT" />
					</key>
					<value>0.012</value>
				</entry>
				<entry>
					<key>
						<ref bean="biomassPGT" />
					</key>
					<value>0.029</value>
				</entry>
				<entry>
					<key>
						<ref bean="nuclearPGT" />
					</key>
					<value>0.133</value>
				</entry>
				<entry>
					<key>
						<ref bean="windPGT" />
					</key>
					<value>0.164</value>
				</entry>
